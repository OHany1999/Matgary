6- datasource


abstract class Base***RemoteDataSource {
  Future<***Model> get***Data(***Parameters Parameters);
}


class ***RemoteDataSource extends Base***RemoteDataSource {
  @override
  Future<***Model> getLoginData(***Parameters Parameters) async {
    Dio dio = Dio();
    final response = await dio.post(
      ApiConstance.loginPath,
      data: {"email": loginParameters.email, "password": loginParameters.password},
      queryParameters: {'Content-Type': 'application/json'},
    );
      return LoginModel.fromJson(response.data);

  }
}
