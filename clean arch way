3- useCase

class Get***UseCase extends BaseUseCase<***Entity,NoParameters or ***Parameters>{
  final Base***Repository base***Repository;
  Get***UseCase(this.base***Repository);

  @override
  Future<Either<Failure, ***Entity>> call(***Parameters Parameters or none)async {
    return await baseHomeRepository.getHomeData(Parameters or none);
  }
}


class ***Parameters extends Equatable {
  final String email;
  final String password;

  const LoginParameters({required this.email, required this.password});

  @override
  List<Object?> get props => [email, password];
}


