5- repository



class ***Repository extends Base***Repository{
  final Base***RemoteDataSource base***RemoteDataSource;

  ***Repository(this.base***RemoteDataSource);

  @override
  Future<Either<Failure, ****Entity>> get***Data(****Parameters Parameters)async {
    try{
      final result = await base***RemoteDataSource.get***Data(Parameters);
      return Right(result);
    }on Exception catch(error){
      return left(Failure(error.hashCode, error.toString()));
    }
  }
}



